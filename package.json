    "js-base64": "^3.7.7",
    "vue": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.2",
    "chrome-types": "^0.1.301",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "vite-plugin-importmap": "^0.1.1",
    "vue-tsc": "^2.0.29"
  }
}